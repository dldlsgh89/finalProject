<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="kr.or.nationRental.administrator.service.InjeungMapper">

	<select id="selectInjeung"
			parameterType="kr.or.nationRental.administrator.service.InjeungDto"
			resultType="kr.or.nationRental.administrator.service.InjeungDto">
			SELECT
				administrator_injeung_num as adminInjeungNum
	 		FROM 
	 			administrator_injeung_num
	 		WHERE 
	 			administrator_injeung_num = #{adminInjeungNum} 		
	</select>

</mapper>